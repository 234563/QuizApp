@model List<QuizApp.Enities.User>
@{
    ViewData["Title"] = "Home Page";
}


<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Total Wins</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.TotalWins</td>
            </tr>
        }
    </tbody>
</table>
